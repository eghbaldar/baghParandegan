                                <DataLabels>
                                    <Border Color="Transparent" />
                                    <Background Color="Transparent" />
                                </DataLabels>
                                <Shadow Color="Wheat" />
                            </Web:PieChart>
                        </Charts>
                        <YTitle StringFormat="Center,Near,Character,DirectionVertical" Font="Tahoma, 8pt, style=Bold"
                            ForeColor="204, 204, 255"></YTitle>
